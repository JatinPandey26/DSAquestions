	public static void main (String[] args) {
		int n = 5;
		List<Integer> res = null;
		List<Integer> temp = null;
		
		for(int i = 0 ; i < n ; i++){
		    
		    res = new ArrayList<Integer>();
		    for(int j = 0 ; j <= i ; j++){
		       if(j == 0 || j == i) {
		           res.add(1);
		       }
		       else{ res.add(temp.get(j-1) + temp.get(j) );}}
		     temp = res;
		   
		    
		      System.out.println(res);
		}
