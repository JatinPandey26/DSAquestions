   public static void sortArrayByParity(int[] arr) {
    int i = 0;
    int j = 0;
    
    while( j < arr.length){
     
        
        if(arr[j] % 2 == 1) j++;
        else{
            int temp = arr[j];
            arr[j] = arr[i];
            arr[i] = temp;
            i++;
            j++;
        }
        
        
    }
    
    
    }
